@model DualGlobe.ERP.Models.TimesheetModel


@{
    ViewBag.Title = "Timesheet";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Timesheet Details
    </h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index", "Home")"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Timesheet Details</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div id="divSearch">
        @{Html.RenderPartial("Search", Model);}
    </div>
    
    
    @if (Model.IsPublicHoliday)
    {
        <div id="divHoliday">
            @{Html.RenderPartial("Holiday", Model);}
        </div>
    }
    else
    {
        <div id="divTimesheet">
            @{Html.RenderPartial("Timesheet", Model);}
        </div>
        
    }

</section>
<!-- /.content -->

